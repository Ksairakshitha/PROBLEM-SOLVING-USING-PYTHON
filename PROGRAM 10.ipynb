    "import math\n",
    "def sin(x,n):\n",
    "    sine=0\n",
    "    for i in range(n):\n",
    "       sign=(-1)**i\n",
    "       pi=22/7\n",
    "       y=x*(pi/180)\n",
    "       sine+=((y**(2.0*i+1)))/(math.factorial(2*i+1))*sign\n",
    "       return sine\n",
    "x=6\n",
    "n=40\n",
    "print(round(sin(x,n),2))\n",
    "    "
   ]
