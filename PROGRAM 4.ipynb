    "from math import factorial\n",
    "n=int(input(\"Enter the no. of rows\"))\n",
    "for i in range(n):\n",
    "    for j in range(n-i+1):\n",
    "         print(end=\" \")\n",
    "    for j in range (i+1):\n",
    "            print(factorial(i)//(factorial(j)*factorial(i-j)),end=\" \")\n",
    "    print()            "
   ]
